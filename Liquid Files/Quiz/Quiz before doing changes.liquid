  function getSelectedRadioValue(name) {
    const radios = document.getElementsByName(name);
    for (let i = 0; i < radios.length; i++) {
      if (radios[i].checked) {
        return radios[i].value;
      }
    }
    return null;
  }

  function getSelectedRadioLabel(name) {
    const radios = document.getElementsByName(name);
    for (let i = 0; i < radios.length; i++) {
      if (radios[i].checked) {
        return radios[i].parentNode.textContent.trim();
      }
    }
    return null;
  }

  function convertToGrams(packSize) {
    if (packSize.includes("x")) {
      const multiplier = parseInt(packSize.split("x")[0].trim());
      const weight = parseFloat(packSize.split("x")[1].trim());
      return multiplier * weight * 1000;
    } else if (packSize.includes("kg")) {
      const weight = parseFloat(packSize.split("kg")[0].trim());
      return weight * 1000;
    } else if (packSize.includes("g")) {
      return parseFloat(packSize.split("g")[0].trim());
    }
    return 0;
    }


    function resetForm() {
        document.getElementById("calculatorForm").reset();
        document.getElementById("output").innerHTML = "";
        const field = document.getElementsByClassName("validation");
            for (let i = 0; i < field.length; i++) {
        field[i].classList.remove("error-border");
      }
    }


    var select = document.getElementById("dogWeight");

    for (var i = 1; i <= 100; i++) {
        var option = document.createElement("option");
        option.value = i;
        option.text = i + "kg";
        select.appendChild(option);
    }


    function highlightField(fieldName) {
      const field = document.getElementsByClassName(fieldName);
      console.log(field.textContent);
      for (var i = 0; i < field.length; i++) {
        field[i].classList.add('error-border');
      }    
    }

    function resetFieldHighlighting(fieldName) {
      const field = document.getElementsByClassName(fieldName);
      for (let i = 0; i < field.length; i++) {
        field[i].classList.remove("error-border");
      }
    }
  
    // drop down

    var input = document.querySelector('#quantity');
    var btnminus = document.querySelector('.qtyminus');
    var btnplus = document.querySelector('.qtyplus');
    
    if (input !== undefined && btnminus !== undefined && btnplus !== undefined && input !== null && btnminus !== null && btnplus !== null) {
    	
    	var min = Number(input.getAttribute('min'));
    	var max = Number(input.getAttribute('max'));
    	var step = Number(input.getAttribute('step'));
    
    	function qtyminus(e) {
    		var current = Number(input.value);
    		var newval = (current - step);
    		if(newval < min) {
    			newval = min;
    		} else if(newval > max) {
    			newval = max;
    		} 
    		input.value = Number(newval);
    		e.preventDefault();
    	}
    
    	function qtyplus(e) {
    		var current = Number(input.value);
    		var newval = (current + step);
    		if(newval > max) newval = max;
    		input.value = Number(newval);
    		e.preventDefault();
    	}
    		
    	btnminus.addEventListener('click', qtyminus);
    	btnplus.addEventListener('click', qtyplus);
      
    } // End qty button

    
    
