        resetFieldHighlighting("dogWeight");
      }

      if (isNaN(activityLevel)) {
        hasError = true;
        highlightField("activityLevel");
      }else {
        resetFieldHighlighting("activityLevel");
      }

      if (isNaN(bodyCondition)) {
        hasError = true;
        highlightField("bodyCondition");
      }else {
        resetFieldHighlighting("bodyCondition");
      }

      if (isNaN(recipeDensity)) {
        hasError = true;
        highlightField("recipe");
      } else {
        resetFieldHighlighting("recipe");
      }

      if (isNaN(feedingLevel)) {
        hasError = true;
        highlightField("feedingLevel");
      }else {
        resetFieldHighlighting("feedingLevel");
      }

      // Check other mandatory fields

      if (hasError == true) {
        errorElement.innerHTML = "Please fill in all mandatory fields.";
        return;
      } else {

        // Perform the calculations
    caloriesPerDay = Math.ceil(activityLevel * 90 * Math.pow(dogWeight, bodyCondition));
    console.log(caloriesPerDay);
    const gramsPerDay = Math.ceil(caloriesPerDay / recipeDensity * feedingLevel / 10) * 10;
    console.log(gramsPerDay);
    let packSize = "";
    if (gramsPerDay >= 150) {
      packSize = "3 x 1.8kg";
    } else if (gramsPerDay >= 90) {
      packSize = "2 x 1.8kg";
    } else if (gramsPerDay >= 40) {
      packSize = "1.8kg";
    } else if (gramsPerDay >= 0) {
      packSize = "800g";
    }
    const totalGrams = convertToGrams(packSize);
    const daysFoodLast = Math.floor(totalGrams / gramsPerDay);
    console.log("Pack Size " + packSize + "  - Total grams " + totalGrams + " - Grams Per Day " + gramsPerDay)

    // Display the results
    const output = document.getElementById("output");
    output.innerHTML = `
